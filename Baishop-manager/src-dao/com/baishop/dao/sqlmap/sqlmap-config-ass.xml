<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMapConfig
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"  
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

	<!-- useStatementNamespaces 是否启用名称空间 --> 
	<!-- cacheModelsEnabled 是否启用缓存机制 -->  
	<!-- lazyLoadingEnabled 是否启用延迟加载机制 -->  
	<!-- enhancementEnabled 是否启用字节码增强机制 -->  
	<!-- errorTracingEnabled 是否启用错误处理机制 -->	
	<settings 
		useStatementNamespaces="true" 
		cacheModelsEnabled ="true" 
		lazyLoadingEnabled="false" 
		enhancementEnabled="true"  
		errorTracingEnabled="true"		
	/>	
	
	<!-- ========================= 系统库 ass ========================= -->
	
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Admins.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Roles.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Modules.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Depts.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/AdminsRoles.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/AdminsDepts.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/AdminsModules.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/RolesModules.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/RemoteAuths.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Params.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/Enums.sqlmap.xml"/>
	<sqlMap resource="com/baishop/dao/sqlmap/ass/City.sqlmap.xml"/>
	
</sqlMapConfig>
